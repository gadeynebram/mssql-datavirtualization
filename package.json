{
  "name": "mssql-datavirtualization",
  "displayName": "MSSQL Data Virtualization Wizard",
  "description": "A VS Code extension to virtualize data using MSSQL APIs.",
  "version": "0.1.1",
  "publisher": "BramGadeyne",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/gadeynebram/mssql-datavirtualization.git"
  },
  "bugs": {
    "url": "https://github.com/gadeynebram/mssql-datavirtualization/issues"
  },
  "keywords": [
    "sql",
    "mssql",
    "sql server",
    "polybase",
    "data virtualization",
    "external tables"
  ],
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mssql-datavirtualization.virtualizeDataWizard"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mssql-datavirtualization.virtualizeDataWizard",
        "title": "Virtualize Data Wizard"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@vscode/vsce": "^3.2.1",
    "typescript": "^5.9.3"
  },
  "extensionDependencies": [
    "ms-mssql.mssql"
  ]
}
