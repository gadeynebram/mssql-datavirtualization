{
  "name": "mssql-datavirtualization",
  "displayName": "MSSQL Data Virtualization Wizard",
  "description": "A VS Code extension to virtualize data using MSSQL APIs.",
  "version": "0.3.0",
  "publisher": "BramGadeyne",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/gadeynebram/mssql-datavirtualization.git"
  },
  "bugs": {
    "url": "https://github.com/gadeynebram/mssql-datavirtualization/issues"
  },
  "keywords": [
    "sql",
    "mssql",
    "sql server",
    "polybase",
    "data virtualization",
    "external tables"
  ],
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:mssql-datavirtualization-view"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mssql-datavirtualization-container",
          "title": "Data Virtualization",
          "icon": "images/icon.png"
        }
      ]
    },
    "views": {
      "mssql-datavirtualization-container": [
        {
          "id": "mssql-datavirtualization-view",
          "name": "Wizard Actions"
        }
      ]
    },
    "commands": [
      {
        "command": "mssql-datavirtualization.virtualizeDataWizard",
        "title": "Virtualize Data Wizard",
        "icon": "$(database)"
      },
      {
        "command": "mssql-datavirtualization.showHelp",
        "title": "Data Virtualization: Show Help"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@vscode/vsce": "^3.2.1",
    "typescript": "^5.9.3"
  },
  "extensionDependencies": [
    "ms-mssql.mssql"
  ]
}
