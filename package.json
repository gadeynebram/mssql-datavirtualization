{
  "name": "mssql-datavirtualization",
  "displayName": "MSSQL Data Virtualization Wizard",
  "description": "A VS Code extension to virtualize data using MSSQL APIs.",
  "version": "0.3.0",
  "publisher": "BramGadeyne",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/gadeynebram/mssql-datavirtualization.git"
  },
  "bugs": {
    "url": "https://github.com/gadeynebram/mssql-datavirtualization/issues"
  },
  "keywords": [
    "sql",
    "mssql",
    "sql server",
    "polybase",
    "data virtualization",
    "external tables"
  ],
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mssql-datavirtualization.virtualizeDataWizard",
    "onView:mssqlDataVirtualization"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mssql-datavirtualization.virtualizeDataWizard",
        "title": "Virtualize Data Wizard (Classic)",
        "category": "MSSQL Data Virtualization"
      },
      {
        "command": "mssql-datavirtualization.openWizardWebview",
        "title": "Start Data Virtualization Wizard",
        "category": "MSSQL Data Virtualization",
        "icon": "$(rocket)"
      },
      {
        "command": "mssql-datavirtualization.refreshView",
        "title": "Refresh",
        "category": "MSSQL Data Virtualization",
        "icon": "$(refresh)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mssqlDataVirtualizationContainer",
          "title": "MSSQL Data Virtualization",
          "icon": "resources/wizard-icon.svg"
        }
      ]
    },
    "views": {
      "mssqlDataVirtualizationContainer": [
        {
          "id": "mssqlDataVirtualization",
          "name": "Wizard",
          "icon": "resources/wizard-icon.svg",
          "contextualTitle": "Data Virtualization"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "mssql-datavirtualization.refreshView",
          "when": "view == mssqlDataVirtualization",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@vscode/vsce": "^3.2.1",
    "typescript": "^5.9.3"
  },
  "extensionDependencies": [
    "ms-mssql.mssql"
  ]
}
