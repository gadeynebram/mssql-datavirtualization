{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
  "lib": ["ES2020", "dom"],
    "outDir": "out",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["vscode"],
    "typeRoots": [
      "./node_modules/@types",
      "./typings"
    ],
    "moduleResolution": "node"
  },
  "include": ["src"],
  "exclude": ["node_modules", ".vscode-test"]
}
